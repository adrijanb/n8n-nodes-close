{
	"name": "@adrijanx/n8n-nodes-close",
	"version": "1.0.1",
	"description": "n8n node for Close.com CRM integration",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/adrijanbajrami/n8n-nodes-close#readme",
	"author": {
		"name": "Adrijan Bajrami",
		"email": "adrijan.bajrami@everhype.de"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adrijanbajrami/n8n-nodes-close.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/CloseApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Close/Close.node.js"
		]
	},
	"devDependencies": {
		"@jest/globals": "^29.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.29.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"jest": "^29.0.0",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"ts-jest": "^29.0.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"n8n-workflow": "*"
	}
}
